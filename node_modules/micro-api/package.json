{
  "name": "micro-api",
  "description": "Minimal routing layer for building JSON APIs with Zeit's Micro",
  "version": "0.1.6",
  "engines": {
    "node": ">=6"
  },
  "repository": {
    "url": "https://github.com/possibilities/micro-api",
    "type": "git"
  },
  "author": "Mike Bannister <mikebannister@gmail.com>",
  "main": "lib/index.js",
  "files": [
    "lib/index.js"
  ],
  "license": "MIT",
  "scripts": {
    "build": "rimraf lib && babel src --out-dir lib",
    "test": "ava --verbose src/test.js",
    "dev": "ava --verbose --watch src/test.js",
    "prepublish": "yarn build",
    "lint": "standard",
    "pretest": "standard"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "standard": "^9.0.2",
    "uniloc": "^0.3.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "async-to-gen": "^1.3.0",
    "ava": "^0.18.2",
    "babel-cli": "^6.24.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "micro": "^7.3.0",
    "rimraf": "^2.6.1",
    "test-listen": "^1.0.0"
  },
  "peerDependencies": {
    "micro": "^7.3.0"
  },
  "ava": {
    "failFast": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "latest",
      "stage-0"
    ]
  }
}
